{
    "name": "jschreuder/bookmark-bureau",
    "description": "A personal bookmarks database that allows creation of dashboards with them.",
    "type": "application",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "Jelmer Schreuder",
            "email": "github@jelmerschreuder.nl"
        }
    ],
    "require": {
        "jschreuder/middle": "dev-master",
        "jschreuder/middle-di": "^1.0",
        "laminas/laminas-diactoros": "^3.8",
        "laminas/laminas-httphandlerrunner": "^2.13",
        "monolog/monolog": "^3.9",
        "robmorgan/phinx": "^0.16.10",
        "symfony/routing": "^7.3",
        "symfony/console": "^7.3",
        "ramsey/uuid": "^4.9",
        "respect/validation": "^2.4"
    },
    "require-dev": {
        "pestphp/pest": "^4.1",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "jschreuder\\BookmarkBureau\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "test": "pest"
    }
}
